{
  "name": "@melonproject/protocol",
  "version": "1.0.12",
  "description": "Technology Regulated and Operated Investment Funds",
  "author": "Melon Council DAO <team@meloncouncil.io>",
  "license": "GPL-3.0",
  "homepage": "https://melonprotocol.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melonproject/protocol.git"
  },
  "bugs": {
    "url": "https://github.com/melonproject/protocol/issues"
  },
  "keywords": [
    "Defi",
    "Ethereum",
    "Melon",
    "Smart Contracts",
    "Solidity"
  ],
  "scripts": {
    "download-thirdparty": "node scripts/download-thirdparty",
    "codegen": "crestproject codegen \"build/contracts/**.json\" tests-new/contracts",
    "compile": "truffle compile",
    "postcompile": "yarn download-thirdparty",
    "deploy": "truffle migrate --skip-dry-run",
    "devchain": "ts-node --require tsconfig-paths/register tests/ganache/fork",
    "format": "prettier --write {scripts,tests}/{*,**/*}.ts",
    "typecheck": "tsc --noEmit",
    "lint": "solhint src/**/*.sol",
    "test": "yarn jest --selectProjects old",
    "test-new": "yarn jest --selectProjects unit"
  },
  "devDependencies": {
    "@0x/order-utils": "^8.5.0-beta.4",
    "@0x/order-utils-v2": "npm:@0x/order-utils@8.4.0",
    "@0x/subproviders": "^6.0.3",
    "@0x/types": "^2.5.0-beta.3",
    "@0x/types-v2": "npm:@0x/types@2.4.3",
    "@0x/utils": "^5.1.2",
    "@airswap/order-utils": "^0.3.20",
    "@nomiclabs/buidler": "^1.3.8",
    "@openzeppelin/contracts": "3.0.1",
    "@types/jest": "^26.0.3",
    "ethers": "^5.0.5",
    "ganache-core": "^2.11.0-beta.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "request-promise": "^4.2.2",
    "solhint": "^3.0.0",
    "truffle": "^5.1.32",
    "ts-jest": "^26.1.1",
    "typescript": "^4.0.0-beta",
    "web3": "1.2.9",
    "web3-eth-abi": "1.2.9",
    "web3-utils": "1.2.9"
  },
  "engines": {
    "node": "^12"
  },
  "jest": {
    "projects": [
      {
        "displayName": "old",
        "preset": "ts-jest",
        "roots": [
          "<rootDir>/tests"
        ],
        "testEnvironment": "<rootDir>/tests/jest.env.js",
        "setupFilesAfterEnv": [
          "<rootDir>/tests/jest.setup.js"
        ],
        "transform": {
          "^.+\\.[jt]s$": "ts-jest"
        },
        "moduleNameMapper": {
          "~/config": "<rootDir>/config",
          "~/(.*)": "<rootDir>/tests/$1"
        },
        "globals": {
          "ts-jest": {
            "diagnostics": false
          }
        }
      },
      {
        "displayName": "unit",
        "preset": "@crestproject/crestproject",
        "roots": [
          "<rootDir>/tests-new/unit"
        ]
      }
    ]
  },
  "dependencies": {
    "@crestproject/crestproject": "^0.0.13"
  }
}
