+++ b/tests/jest.env.js
@@ -0,0 +1,47 @@
+const ganache = require('ganache-core');
+const utils = require('web3-utils');
+const Web3 = require('web3');
+const NodeEnvironment = require('jest-environment-node');
+
+class MelonEnvironment extends NodeEnvironment {
+  constructor(config, context) {
+    super(config, context);
+
+    this.forkStartingBalance = this.global.forkStartingBalance || utils.toWei('10000000', 'ether');
+    this.forkSource = this.global.forkSource || 'http://127.0.0.1:8545';
+    this.forkGasLimit = this.global.forkGasLimit || '0x989680';
+    this.forkPrivateKeys = this.global.forkPrivateKeys || [];
+    this.forkUnlockedAccounts = this.global.forkUnlockedAccounts || [];
+    this.forkMnemonic = this.global.forkMnemonic || 'exhibit now news planet fame thank swear reform tilt accident bitter axis';
+    this.forkNetworkId = this.global.forkNetworkId || 1;
+  }
+
+  async setup() {
+    await super.setup();
+    
+    const provider = ganache.provider({
+      fork: this.forkSource,
+      mnemonic: this.forkMnemonic,
+      network_id: this.forkNetworkId,
+      gasLimit: this.forkGasLimit,
+      unlocked_accounts: this.forkUnlockedAccounts,
+      accounts: this.forkPrivateKeys.map(key => ({
+        secretKey: key,
+        balance: this.forkStartingBalance,
+      })),
+    });
+  
+    this.global.web3 = new Web3(provider, null, {
+      transactionConfirmationBlocks: 1
+    });
+
+    this.forkPrivateKeys.forEach(key => this.global.web3.eth.accounts.wallet.add(key));
+  }
+
+  async teardown() {
+    // TODO: Should we close the ganache provider?
+    await super.teardown();
+  }
+}
+
+module.exports = MelonEnvironment;
